<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yvision">
    <!-- 相机权限 -->
    <uses-permission android:name="android.permission.CAMERA" />
    <!-- wifi权限-->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>

    <!-- 网络权限 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <!-- 极光权限设置开始-->
    <!-- Required -->
    <permission
        android:name="com.yvision.permission.JPUSH_MESSAGE"
        android:protectionLevel="signature" />

    <!-- Required  一些系统要求的权限，如访问网络等-->
    <uses-permission android:name="com.yvision.permission.JPUSH_MESSAGE" />
    <uses-permission android:name="android.permission.RECEIVE_USER_PRESENT" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />

    <!-- Optional for location -->
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" /> <!-- 用于开启 debug 版本的应用在6.0 系统上 层叠窗口权限 -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <!-- 极光权限设置结束-->

    <application
        android:name="com.yvision.common.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <!-- 管理界面  MainActivity -->
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:label="@string/app_name"
            >

        </activity>
        <!-- 登录 LoginActivity-->
        <activity
            android:name="com.yvision.LoginActivity"
            android:screenOrientation="portrait"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- 人脸登录 LoginByFaceActivity -->
        <activity
            android:name="com.yvision.LoginByFaceActivity"
            android:screenOrientation="portrait"
            >
        </activity>
        <!-- MyCameraActivity -->
        <activity
            android:name="com.yvision.MyCameraActivity"
            android:screenOrientation="portrait"
            >
        </activity>

        <!-- MyChangeCameraActivity 无标题-->
        <activity
            android:name="com.yvision.MyChangeCameraActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Anim_style3"
            >
        </activity>

        <!-- PhotoClipPictureActivity -->
        <activity
            android:name="com.yvision.PhotoClipPictureActivity"
            android:screenOrientation="portrait"
            >
        </activity>
        <!-- 人脸登记 -->
        <activity
            android:name="com.yvision.AddMyFaceActivity"
            android:screenOrientation="portrait"
            android:label="@string/app_name" >
        </activity>
        <!--wifi考勤设置 -->
        <activity
            android:name="com.yvision.WifiSettingActivity"
            android:screenOrientation="portrait"
            android:label="@string/app_name" >
        </activity>
        <!--url设置 -->

        <!--新员工注册界面 -->
        <activity
            android:name="com.yvision.AddNewEmployeeActivity"
            android:screenOrientation="portrait">
        </activity>

        <!--老员工注册界面 -->
        <activity
            android:name="com.yvision.AddOldEmployeeActivity"
            android:screenOrientation="portrait">
        </activity>

        <!-- 修改密码 -->
        <activity
            android:name="com.yvision.ChangePassWordActivity"
            android:screenOrientation="portrait">
        </activity>

        <!-- 设置 -->
        <activity
            android:name="com.yvision.SettingActivity"
            android:screenOrientation="portrait">
        </activity>

        <!-- 访客系统主界面 -->
        <activity
            android:name="com.yvision.MainVisitorActivity"
            android:screenOrientation="portrait"
            >
        </activity>

        <!-- 注册系统系统主界面 -->
        <activity
            android:name="com.yvision.MainRegisterActivity"
            android:screenOrientation="portrait"
            >
        </activity>

        <!-- 考勤系统主界面 -->
        <activity
            android:name="com.yvision.MainUserActivity"
            android:screenOrientation="portrait"
            >
        </activity>

        <!-- 考勤详细界面 -->
        <activity
            android:name="com.yvision.UserInfoActivity"
            android:screenOrientation="portrait"
            >
        </activity>

        <!-- 添加访客 -->
        <activity
            android:name="com.yvision.AddVisitorFaceActivity"
            android:screenOrientation="portrait"
            >
        </activity>

        <!-- 地图考勤 -->
        <activity
            android:name="com.yvision.MapAttendedActivity"
            android:screenOrientation="portrait"
            >
        </activity>

        <!-- 访客详情 -->
        <activity
            android:name="com.yvision.VisitorInfoActivity"
            android:screenOrientation="portrait"
            >
        </activity>

        <!-- 极光application配置开始-->
        <!-- Rich push 核心功能 since 2.0.6-->
        <activity
            android:name="cn.jpush.android.ui.PopWinActivity"
            android:theme="@style/MyDialogStyle"
            android:exported="false">
        </activity>

        <!-- Required SDK核心功能-->
        <activity
            android:name="cn.jpush.android.ui.PushActivity"
            android:configChanges="orientation|keyboardHidden"
            android:theme="@android:style/Theme.NoTitleBar"
            android:exported="false">
            <intent-filter>
                <action android:name="cn.jpush.android.ui.PushActivity" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="com.yvision" />
            </intent-filter>
        </activity>
        <!-- Required  SDK核心功能-->
        <service
            android:name="cn.jpush.android.service.DownloadService"
            android:enabled="true"
            android:exported="false" >
        </service>

        <!-- Required SDK 核心功能-->
        <!-- 可配置android:process参数将PushService放在其他进程中 -->
        <service
            android:name="cn.jpush.android.service.PushService"
            android:process=":mult">
            <intent-filter>
                <action android:name="cn.jpush.android.intent.REGISTER" />
                <action android:name="cn.jpush.android.intent.REPORT" />
                <action android:name="cn.jpush.android.intent.PushService" />
                <action android:name="cn.jpush.android.intent.PUSH_TIME" />
            </intent-filter>
        </service>

        <!-- since 1.8.0 option 可选项。用于同一设备中不同应用的JPush服务相互拉起的功能。 -->
        <!-- 若不启用该功能可删除该组件，将不拉起其他应用也不能被其他应用拉起 -->
        <service
            android:name="cn.jpush.android.service.DaemonService"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="cn.jpush.android.intent.DaemonService" />
                <category android:name="com.yvision" />
            </intent-filter>

        </service>

        <!-- Required SDK核心功能-->
        <receiver
            android:name="cn.jpush.android.service.PushReceiver"
            android:enabled="true">
            <intent-filter android:priority="1000">
                <action android:name="cn.jpush.android.intent.NOTIFICATION_RECEIVED_PROXY" />   <!--Required  显示通知栏 -->
                <category android:name="com.yvision" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.USER_PRESENT" />
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
            <!-- Optional -->
            <intent-filter>
                <action android:name="android.intent.action.PACKAGE_ADDED" />
                <action android:name="android.intent.action.PACKAGE_REMOVED" />

                <data android:scheme="package" />
            </intent-filter>
        </receiver>

        <!-- Required SDK核心功能-->
        <receiver android:name="cn.jpush.android.service.AlarmReceiver" android:exported="false"/>

        <!-- User defined.  For test only  用户自定义的广播接收器-->
        <receiver
            android:name="com.yvision.receiver.MyReceiver"
            android:exported="false"
            android:enabled="true">
            <intent-filter>
                <action android:name="cn.jpush.android.intent.REGISTRATION" /> <!--Required  用户注册SDK的intent-->
                <action android:name="cn.jpush.android.intent.MESSAGE_RECEIVED" /> <!--Required  用户接收SDK消息的intent-->
                <action android:name="cn.jpush.android.intent.NOTIFICATION_RECEIVED" /> <!--Required  用户接收SDK通知栏信息的intent-->
                <action android:name="cn.jpush.android.intent.NOTIFICATION_OPENED" /> <!--Required  用户打开自定义通知栏的intent-->
                <action android:name="cn.jpush.android.intent.CONNECTION" /><!-- 接收网络变化 连接/断开 since 1.6.3 -->
                <category android:name="com.yvision" />
            </intent-filter>
        </receiver>

        <!-- Required  . Enable it you can get statistics data with channel -->
        <meta-data android:name="JPUSH_CHANNEL" android:value="developer-default"/>
        <meta-data android:name="JPUSH_APPKEY" android:value="f2becb1c30528355b9b7a853" /> <!--  </>值来自开发者平台取得的AppKey-->

        <!-- 极光application配置结束-->


    </application>

</manifest>