<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yvision">
    <!-- 相机权限 -->
    <uses-permission android:name="android.permission.CAMERA" />
    <!-- wifi权限-->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <!-- 个推SDK权限配置开始 -->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <!-- 存储权限-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <!-- iBeancon功能与个推3.0电子围栏功能所需要的权限为非必需的可选择权限，可以选择性配置，以便使用个推3.0电子围栏功能 -->
    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>
    <!-- 个推3.0电子围栏功能所需权限 -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <!-- 浮动通知权限 -->
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"/>
    <!-- 自定义权限 -->
    <uses-permission android:name="getui.permission.GetuiService.com.yvision" />
    <!--替换为第三方应用的包名-->
    <permission
        android:name="getui.permission.GetuiService.com.yvision"
        android:protectionLevel="normal" >
    </permission><!--替换为第三方应用的包名-->
    <!-- 个推SDK权限配置结束 -->

    <!-- 网络权限 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:name="com.yvision.common.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <!-- 管理界面  MainActivity -->
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- 登录 LoginActivity-->
        <activity
            android:name="com.yvision.LoginActivity"
            android:screenOrientation="portrait"
            >

        </activity>
        <!-- 人脸登录 LoginByFaceActivity -->
        <activity
            android:name="com.yvision.LoginByFaceActivity"
            android:screenOrientation="portrait"
            >
        </activity>
        <!-- MyCameraActivity -->
        <activity
            android:name="com.yvision.MyCameraActivity"
            android:screenOrientation="portrait"
            >
        </activity>

        <!-- MyChangeCameraActivity 无标题-->
        <activity
            android:name="com.yvision.MyChangeCameraActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Anim_style3"
            >
        </activity>

        <!-- PhotoClipPictureActivity -->
        <activity
            android:name="com.yvision.PhotoClipPictureActivity"
            android:screenOrientation="portrait"
            >
        </activity>
        <!-- 人脸登记 -->
        <activity
            android:name="com.yvision.AddMyFaceActivity"
            android:screenOrientation="portrait"
            android:label="@string/app_name" >
        </activity>
        <!--wifi考勤设置 -->
        <activity
            android:name="com.yvision.WifiSettingActivity"
            android:screenOrientation="portrait"
            android:label="@string/app_name" >
        </activity>
        <!--url设置 -->

        <!--新员工注册界面 -->
        <activity
            android:name="com.yvision.AddNewEmployeeActivity"
            android:screenOrientation="portrait">
        </activity>

        <!--老员工注册界面 -->
        <activity
            android:name="com.yvision.AddOldEmployeeActivity"
            android:screenOrientation="portrait">
        </activity>

        <!-- 修改密码 -->
        <activity
            android:name="com.yvision.ChangePassWordActivity"
            android:screenOrientation="portrait">
        </activity>

        <!-- 设置 -->
        <activity
            android:name="com.yvision.SettingActivity"
            android:screenOrientation="portrait">
        </activity>

        <!-- 访客系统主界面 -->
        <activity
            android:name="com.yvision.MainVisitorActivity"
            android:screenOrientation="portrait"
            >
        </activity>

        <!-- 注册系统系统主界面 -->
        <activity
            android:name="com.yvision.MainRegisterActivity"
            android:screenOrientation="portrait"
            >
        </activity>

        <!-- 考勤系统主界面 -->
        <activity
            android:name="com.yvision.MainUserActivity"
            android:screenOrientation="portrait"
            >
        </activity>

        <!-- 考勤详细界面 -->
        <activity
            android:name="com.yvision.UserInfoActivity"
            android:screenOrientation="portrait"
            >
        </activity>

        <!-- 添加访客 -->
        <activity
            android:name="com.yvision.AddVisitorFaceActivity"
            android:screenOrientation="portrait"
            >
        </activity>

        <!-- 地图考勤 -->
        <activity
            android:name="com.yvision.MapAttendedActivity"
            android:screenOrientation="portrait"
            >
        </activity>

        <!-- 访客详情 -->
        <activity
            android:name="com.yvision.VisitorInfoActivity"
            android:screenOrientation="portrait"
            >
        </activity>
        <!-- 个推SDK配置开始 -->
        <!-- 配置的第三方参数属性 -->
        <meta-data
            android:name="PUSH_APPID"
            android:value="81NW0fXkm96iLav9XEzJf6" /> <!-- 替换为第三方应用的APPID -->
        <meta-data
            android:name="PUSH_APPKEY"
            android:value="ATuFOELg446lGtbrQUDQ61" /> <!-- 替换为第三方应用的APPKEY -->
        <meta-data
            android:name="PUSH_APPSECRET"
            android:value="2kh8c2304w6oIhezOfzXe3" /> <!-- 替换为第三方应用的APPSECRET -->
        <!-- 配置SDK核心服务 -->
        <service
            android:name="com.igexin.sdk.PushService"
            android:exported="true"
            android:label="NotificationCenter"
            android:process=":pushservice" >
            <intent-filter>
                <action android:name="com.igexin.sdk.action.service.message"/>
            </intent-filter>
        </service>

        <service
            android:name="com.igexin.sdk.PushServiceUser"
            android:exported="true"
            android:label="NotificationCenterUser">
            <intent-filter>
                <action android:name="com.igexin.sdk.action.user.message"/>
            </intent-filter>
        </service>
        <receiver android:name="com.igexin.sdk.PushReceiver" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
                <action android:name="android.intent.action.USER_PRESENT" />
                <action android:name="com.igexin.sdk.action.refreshls" />
                <!-- 以下三项为可选的action声明，可大大提高service存活率和消息到达速度 -->
                <action android:name="android.intent.action.MEDIA_MOUNTED" />
                <action android:name="android.intent.action.ACTION_POWER_CONNECTED" />
                <action android:name="android.intent.action.ACTION_POWER_DISCONNECTED" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="com.igexin.sdk.PushManagerReceiver"
            android:exported="false" >
            <intent-filter>
                <action android:name="com.igexin.sdk.action.pushmanager" />
            </intent-filter>
        </receiver>
        <activity
            android:name="com.igexin.sdk.PushActivity"
            android:excludeFromRecents="true"
            android:exported="false"
            android:process=":pushservice"
            android:taskAffinity="com.igexin.sdk.PushActivityTask"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity
            android:name="com.igexin.sdk.GActivity"
            android:excludeFromRecents="true"
            android:exported="true"
            android:process=":pushservice"
            android:taskAffinity="com.igexin.sdk.PushActivityTask"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <service
            android:name="com.igexin.download.DownloadService"
            android:process=":pushservice" />
        <receiver android:name="com.igexin.download.DownloadReceiver" >
            <intent-filter>
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
        </receiver>
        <provider
            android:name="com.igexin.download.DownloadProvider"
            android:authorities="downloads.com.yvision.yvision0829"
            android:exported="true"
            android:process=":pushservice" />

        <!-- 配置第三方Receiver -->
        <receiver
            android:name=".receiver.GetuiReceiver"
            android:exported="false">
            <intent-filter>
                <action android:name="com.igexin.sdk.action.81NW0fXkm96iLav9XEzJf6" />
            </intent-filter>
        </receiver><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version"/>

        <!-- 个推SDK配置结束 -->
    </application>

</manifest>